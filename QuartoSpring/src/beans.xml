<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
 
	<bean id="ImpresaRistrutturazioneService" class="it.prova.service.ImpresaRistrutturazioneServiceImpl">
		<property name="idraulicoService" ref="IdraulicoService" />
		<property name="muratoreService" ref="MuratoreService" />
		<property name="elettricistaService" ref="ElettricistaService" />
		<property name="imbianchinoService" ref="ImbianchinoService" />
	</bean>
	
	<bean id="IdraulicoService" class="it.prova.service.IdraulicoServiceImpl">
	</bean>
	
	<bean id="MuratoreService" class="it.prova.service.MuratoreServiceImpl">
	</bean>
	
	<bean id="ElettricistaService" class="it.prova.service.ElettricistaServiceImpl">
	</bean>
	
	<bean id="ImbianchinoService" class="it.prova.service.ImbianchinoServiceImpl">
	</bean>
 
</beans>